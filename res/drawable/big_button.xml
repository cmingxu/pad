<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#56797f" />
            <gradient  android:angle="-90"  android:startColor="#51797f" android:endColor="#52797f"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#53397f" />
            <solid  android:color="#548d9e"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#55797f" />
            <gradient  android:angle="-90"  android:startColor="#63c5d3" android:endColor="#658d9e" />
        </shape>
    </item>
</selector>