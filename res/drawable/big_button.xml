<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#00797f" />
            <gradient  android:angle="-90"  android:startColor="#00797f" android:endColor="#00797f"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#00397f" />
            <solid  android:color="#008d9e"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#00797f" />
            <gradient  android:angle="-90"  android:startColor="#00c5d3" android:endColor="#008d9e" />
        </shape>
    </item>
</selector>